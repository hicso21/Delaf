{
    "expo": {
        "name": "DELAF",
        "slug": "app",
        "version": "1.11.11",
        "orientation": "portrait",
        "icon": "./assets/DELAF_portada.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/DELAF_portada.png",
            "resizeMode": "contain",
            "backgroundColor": "#1a181d"
        },
        "ios": {
            "supportsTablet": true,
            "splash": {
                "backgroundColor": "#1a181d",
                "resizeMode": "contain",
                "image": "./assets/DELAF_portada.png"
            },
            "bundleIdentifier": "com.hicso.delaf",
            "infoPlist": {
                "UIBackgroundModes": ["audio"]
            }
        },
        "android": {
            "versionCode": 26,
            "softwareKeyboardLayoutMode": "pan",
            "intentFilters": [
                {
                    "action": "VIEW",
                    "data": [
                        {
                            "scheme": "myapp",
                            "host": "checkout",
                            "pathPrefix": "/congrats"
                        }
                    ],
                    "category": ["BROWSABLE", "DEFAULT"]
                }
            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/DELAF_portada.png",
                "backgroundColor": "#1a181d"
            },
            "package": "com.hicso.delaf",
            "splash": {
                "backgroundColor": "#1a181d",
                "resizeMode": "contain",
                "image": "./assets/DELAF_portada.png"
            },
            "blockedPermissions": ["android.permission.READ_MEDIA_IMAGES"]
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-font",
                {
                    "fonts": ["./assets/fonts/IBMPlexSansJP_400Regular.ttf"]
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "bbcc0d35-a75f-4ef6-8abd-8ee6382a4fca"
            }
        }
    }
}
